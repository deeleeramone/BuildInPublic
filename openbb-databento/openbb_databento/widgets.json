  "live_grid_index": {
    "name": "Index Futures Trades (Live Grid)",
    "description": "Live trades feed for index futures.",
    "type": "live_grid",
    "endpoint": "live/get_ws_data",
    "wsEndpoint": "live/ws",
    "source": [
      "Databento",
      "CME"
    ],
    "data": {
      "wsRowIdColumn": "symbol",
      "table": {
        "showAll": true,
        "columnsDefs": [
          {
            "field": "date",
            "headerName": "Date",
            "type": "date"
          },
          {
            "field": "symbol",
            "headerName": "Symbol",
            "type": "text"
          },
          {
            "field": "name",
            "headerName": "Name",
            "type": "text"
          },
          {
            "field": "side",
            "headerName": "Side",
            "type": "text"
          },
          {
            "field": "size",
            "headerName": "Size",
            "type": "number"
          },
          {
            "field": "price",
            "headerName": "Price",
            "type": "number"
          }
        ]
      }
    },
    "params": [
      {
        "paramName": "symbol",
        "description": "The symbol to get details for",
        "value": null,
        "label": "Symbol",
        "type": "endpoint",
        "multiSelect": true,
        "optionsEndpoint": "live/symbology",
        "optionsParams": {
          "asset_type": "index"
        },
        "style": {
          "popupWidth": 400
        }
      }
    ],
    "gridData": {
      "w": 25,
      "h": 15
    }
  },
