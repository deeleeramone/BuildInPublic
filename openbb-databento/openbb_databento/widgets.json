          },
          {
            "field": "symbol",
            "headerName": "Symbol",
            "type": "text"
          },
          {
            "field": "name",
            "headerName": "Name",
            "type": "text"
          },
          {
            "field": "side",
            "headerName": "Side",
            "type": "text"
          },
          {
            "field": "size",
            "headerName": "Size",
            "type": "number"
          },
          {
            "field": "price",
            "headerName": "Price",
            "type": "number"
          }
        ]
      }
    },
    "params": [
      {
        "paramName": "symbol",
        "description": "The symbol to get details for",
        "value": null,
        "label": "Symbol",
        "type": "endpoint",
        "multiSelect": true,
        "optionsEndpoint": "live/symbology",
        "optionsParams": {
          "asset_type": "index"
        },
        "style": {
          "popupWidth": 400
        }
      }
    ],
    "gridData": {
      "w": 25,
